{
  "name": "yuncms/yii2-wallet",
  "description": "wallet",
  "keywords": [
    "wallet",
    "yii2"
  ],
  "type": "yii2-extension",
  "license": "BSD-3-Clause",
  "support": {
    "issues": "https://github.com/yuncms/yii2-wallet/issues",
    "source": "https://github.com/yuncms/yii2-wallet"
  },
  "authors": [
    {
      "name": "Xu TL",
      "email": "xutongle@gmail.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "~2.0.6",
    "yuncms/yii2-user": "~2.0"
  },
  "autoload": {
    "files": [
      "helpers.php"
    ],
    "psr-4": {
      "yuncms\\wallet\\": ""
    }
  },
  "suggest": {
    "yuncms/yii2-payment": "Allows you to use Payment"
  },
  "extra": {
    "yuncms": {
      "name": "wallet",
      "i18n": {
        "class": "yii\\i18n\\PhpMessageSource",
        "basePath": "@yuncms/wallet/message"
      },
      "migrationNamespace": "yuncms\\wallet\\migrations",
      "backend": {
        "class": "yuncms\\wallet\\backend\\Module"
      },
      "frontend": {
        "class": "yuncms\\wallet\\frontend\\Module"
      }
    },
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  ]
}
